{
    "name": "nozavroni/csvelte",
    "description": "Slender, elegant CSV for PHP",
    "type": "library",
    "keywords": ["CSV", "PHP", "PHP5", "CSVW", "data-interchange", "JSON", "CSV Schema", "CSV Validation", "CSV Dialect", "Excel", "Tabular Data", "RFC 4180"],
    "homepage": "https://deni-zen.github.io/csvelte/",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Visinoni",
            "email": "luke.visinoni@gmail.com",
            "homepage": "http://luke.visinoni.net/"
        }
    ],
    "support": {
        "issues": "https://github.com/nozavroni/csvelte/issues"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "nozavroni/collect": "^0.1.1",
        "danielstjules/stringy": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mikey179/vfsStream": "^1.6",
        "fzaninotto/Faker": "^1.6",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "CSVelte\\": "src/CSVelte"
        },
        "files": ["src/CSVelte/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "org\\": "vendor/mikey179/vfsStream/src/main/php",
            "CSVelteTest\\": "tests/CSVelte"
        }
    }
}
